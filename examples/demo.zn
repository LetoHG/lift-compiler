func main() {
  return max(7, 5);
}

/* find the maximum of to numbers */
func max(arg1 : i32, arg2 : u8) -> i32 {
  let ret : i32 = 0;

  return add_with_while_loop(arg1, arg2);

  for i in 0..10 {
    if i > 5 {
      return i;
    }
  }
  return ret;

  if arg1 > arg2 {
    arg1 += 5;
    return arg1 + 2 * 26;
  } else {
    return arg2;
  }
}

func add_with_while_loop(a: u32, b: u32) {
  while a > 0 {
    b += 1;
    a -= 1;
  }
  return b;
}


return main();
